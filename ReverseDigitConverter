def digitize(n):
    listN = []
    for number in n:
        listN.append(number)
    result = []
    for num in reversed(listN):
        result.append(int(num))
    print(result)


def master():
    n = input("Input some numbers you want to reverse (format: 23456): ")
    try:
        digitize(n)
    except:
        print("Invalid input")

master()
